{
  "name": "obey-kids",
  "version": "1.0.0",
  "scripts": {
    "start": "node server.js",
    "lint": "esw --color --watch src test",
    "bundle": "NODE_ENV=\"production\" webpack -p --config webpack.config.js",
    "test": "esw --color src test && jest",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage"
  },
  "jest": {
    "setupFiles": [
      "./jest.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "assets"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cedmax/obey-kids.git"
  },
  "homepage": "http://obk.it",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.1.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "css-loader": "^0.26.2",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^2.13.1",
    "extract-text-webpack-plugin": "1.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "node-sass": "^4.5.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.2",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.2",
    "webpack": "^1.14.0",
    "webpack-combine-loaders": "^2.0.3",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "autobind-decorator": "^1.3.4",
    "blankshield": "^0.6.1",
    "classnames": "^2.2.5",
    "convert-string": "^0.1.0",
    "digital-root": "^1.0.4",
    "eslint-plugin-react": "^6.10.0",
    "eslint-watch": "^3.0.1",
    "firebase": "^3.7.0",
    "firebase-rsvp": "^0.0.7",
    "mobase": "^0.1.2",
    "mobx": "^3.0.0",
    "mobx-react": "^4.1.0",
    "mobx-react-devtools": "^4.2.11",
    "moment": "^2.17.1",
    "promise-polyfill": "^6.0.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router": "^3.0.2",
    "react-sound": "^0.5.2",
    "react-svg-inline": "^1.2.0"
  }
}
